{
  "extends": [
    "development"
  ],
  "hints": {
    "typescript-config/is-valid": "off",
    "typescript-config/consistent-casing": "off",
    "typescript-config/strict": "off",
    "button-type": "off",
    "axe/structure": [
      "default",
      {
        "list": "off"
      }
    ],
    "compat-api/css": [
      "default",
      {
        "ignore": [
          "backdrop-filter"
        ]
      }
    ],
    "compat-api/html": [
      "default",
      {
        "ignore": [
          "iframe[loading]"
        ]
      }
    ]
  }
}